include ../test

mixin test


div.container

    ul.nav.nav-tabs
        li
            a(href='#/detail/:{{id}}') Detail
        li
            a(href='#/har/:{{id}}') HAR analysis
        li.active
            a(href='') History

    div.well
        ul.list-unstyled
            li
                strong {{id}}
                |&nbsp;({{timestamp}})
            li {{url}}


    div.panel.panel-default(ng-repeat="f in fields")
        div.panel-heading {{f.name}}
        div.panel-body.text-center
            div(id="{{f.tool}}History").historyChart
                img(src="/images/ajax-loader.gif")
